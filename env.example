# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=my_spring_boot_db
DB_USERNAME=root
DB_PASSWORD=

# JPA Configuration
JPA_DDL_AUTO=update
JPA_SHOW_SQL=true
